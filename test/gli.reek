--- 
UncommunicativeParameterName: 
  accept: S[]

  exclude: []

  enabled: false
  reject: 
  - !ruby/regexp /^.$/
  - !ruby/regexp /[0-9]$/
  - !ruby/regexp /[A-Z]/
LargeClass: 
  max_methods: 25
  exclude: []

  enabled: true
  max_instance_variables: 10
UncommunicativeMethodName: 
  accept: []

  exclude: []

  enabled: true
  reject: 
  - !ruby/regexp /^[a-z]$/
  - !ruby/regexp /[0-9]$/
  - !ruby/regexp /[A-Z]/
LongParameterList: 
  max_params: 5
  exclude: []

  enabled: true
  overrides: 
    initialize: 
      max_params: 6
FeatureEnvy: 
  exclude: &id001 []

  enabled: false
ClassVariable: 
  exclude: *id001
  enabled: false
BooleanParameter: 
  exclude: ['initialize', 'name_as_string']
  enabled: true
IrresponsibleModule: 
  exclude: *id001
  enabled: true
UncommunicativeModuleName: 
  accept: 
  - Inline::C
  exclude: []

  enabled: true
  reject: 
  - !ruby/regexp /^.$/
  - !ruby/regexp /[0-9]$/
NestedIterators: 
  ignore_iterators: []

  exclude:
  - option_parser

  enabled: true
  max_allowed_nesting: 2
LongMethod: 
  max_statements: 10
  exclude: 
  - initialize
  - parse_options
  - run
  - reset
  enabled: true
Duplication: 
  allow_calls: []

  exclude: ['run']

  enabled: true
  max_calls: 3
UtilityFunction: 
  max_helper_calls: 1
  exclude: [ 'find_non_flag_index', 'override_default', 'proceed?', 'command_exists?' ,'option_parser']

  enabled: true
Attribute: 
  exclude: []

  enabled: false
UncommunicativeVariableName: 
  accept: ['_']

  exclude: []

  enabled: true
  reject: 
  - !ruby/regexp /^.$/
  - !ruby/regexp /[0-9]$/
  - !ruby/regexp /[A-Z]/
SimulatedPolymorphism: 
  exclude: []

  enabled: true
  max_ifs: 2
DataClump: 
  exclude: []

  enabled: false
  max_copies: 2
  min_clump_size: 2
ControlCouple: 
  exclude: ['parse_options_helper', 'program_desc', 'program_name', 'name_as_string' ]
  enabled: true
LongYieldList: 
  max_params: 3
  exclude: []

  enabled: true
